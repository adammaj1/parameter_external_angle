kill(all);
remvalue(all);


f(x):=if (x=0) then [0,1] else [1,0];
compile(all);


a:[];
a:endcons([0],a);


for n:2 thru 10 step 1 do (
a:endcons([],a),
for x in a[n-1] do (a[n]:endcons(first(f(x)),a[n]),
                  a[n]:endcons(second(f(x)),a[n])),

print(n,"0.(",a[n], ")")


